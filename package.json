{
  "name": "webpush-solganeo",
  "version": "1.0.0",
  "description": "This project seeks to create a server to send webpush-notifications using SFMC and the journey builder tool.",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && tsc && npm run copy-files",
    "clean": "rm -rf ./dist",
    "postinstall": "tsc",
    "copy-files": "cp -r ./public/ ./dist/ && cp ./api/config/.env.local.json ./dist/api/config/ && cp ./api/swagger.json ./dist/api/",
    "start": " node dist/api/index.js",
    "dev": "ts-node-dev  --respawn ./api/index.ts",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solganeo-org/webpush-solganeo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/solganeo-org/webpush-solganeo/issues"
  },
  "homepage": "https://github.com/solganeo-org/webpush-solganeo#readme",
  "dependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/uuid": "^8.3.4",
    "amqplib": "^0.10.0",
    "body-parser": "^1.20.0",
    "convict": "^6.2.3",
    "convict-format-with-validator": "^6.2.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "fuel-rest": "^3.2.2",
    "fuel-soap": "^2.2.4",
    "jsonwebtoken": "^8.5.1",
    "swagger-ui-express": "^4.4.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.8.2",
    "@types/convict": "^6.1.1",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.43",
    "@types/request": "^2.48.8",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.17.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.7.0",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.3"
  }
}
